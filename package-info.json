{
  "name": "elroute",
  "version": "1.0.0",
  "description": "Gratis offline ruteplanlegger for elbiler",
  "keywords": ["elbil", "rute", "planlegging", "norge", "offline", "tesla", "ev"],
  "author": "ElRoute Team",
  "license": "MIT",
  "homepage": "https://github.com/[din-bruker]/elroute",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/[din-bruker]/elroute.git"
  },
  "bugs": {
    "url": "https://github.com/[din-bruker]/elroute/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "android": "npm run build && npx cap sync android && npx cap run android",
    "ios": "npm run build && npx cap sync ios && npx cap run ios",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap add android && npx cap update android",
    "cap:ios": "npx cap add ios && npx cap update ios"
  }
}